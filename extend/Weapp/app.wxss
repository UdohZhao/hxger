/**app.wxss**/
/* 引如外部css */
@import 'dist/style/weui.wxss';
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
 image{display: initial;} 
 
 .h10{
  display: block;
  font-size: 0;
  height: 10rpx;
  line-height: 0;
  clear: both;
  width: 100%;
  }
.errorMsg{
  width: 100%;
  height: 200px;
  font-size: 20px;
  text-align: center;
}
.errorMsg_img{
  height: 120px;
  width: 120px;
}
.errorMsg_desc{
  font-size: 14px;
  color: #999;
  margin: 10px 0;
}