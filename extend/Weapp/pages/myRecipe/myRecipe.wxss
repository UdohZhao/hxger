.page__hd{
  position: fixed;
  top:0;
  left: 0;
  background-color: #fff;
  height: 80rpx;
  line-height: 80rpx;
  width: 100%;
  text-align: center;
  z-index: 2;
}
.page__title{
  border-bottom: 1px solid #ddd;
  color: #ff8900;
}
.marTop90{
  margin-top: 90rpx;
}
.eye,.star{
  display: inline-block;
  width: 200rpx;
  vertical-align: top;
}
.eye image,.star image{
  width: 34rpx;
  height: 34rpx;
  display: inline-block;
  vertical-align: top;
  margin-right: 20rpx;
}
.weui-panel::before{
  border-top: 0;
}